@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix rating: <https://purl.archive.org/git2rdf/v2/git2RDFLab-rating#> .

#
# ╭──────────────────────────────────────────────────╮
# │ Metadata                                         │
# ╰──────────────────────────────────────────────────╯
#

<https://purl.archive.org/git2rdf/v2/git2RDFLab-rating#>
    a owl:Ontology ;
    dc:title "The git2RDFLab Rating Ontology"@en ;
    dc:description "A structured schema for capturing evaluations, ratings, and assessment context."@en ;
    vann:preferredNamespacePrefix "rating" ;
    vann:preferredNamespaceUri "https://purl.archive.org/git2rdf/v2/git2RDFLab-rating#" ;
    owl:versionInfo "Version 2.2 - 2025-07-20"@en ;
    dc:modified "2025-07-20"^^xsd:date ;
    dc:contributor "https://github.com/git2RDFLab" .

#
# ╭──────────────────────────────────────────────────╮
# │ Core Classes                                     │
# ╰──────────────────────────────────────────────────╯
#

rating:Evaluation a rdfs:Class ;
    rdfs:label "Evaluation"@en ;
    rdfs:comment "A structured assessment process containing multiple ratings."@en .

rating:Rating a rdfs:Class ;
    rdfs:label "Rating"@en ;
    rdfs:comment "A single evaluative judgment within an evaluation."@en .

rating:Agent a rdfs:Class ;
    rdfs:label "Agent"@en ;
    rdfs:comment "An entity capable of performing evaluations."@en .

rating:HumanAgent a rdfs:Class ;
    rdfs:subClassOf rating:Agent ;
    rdfs:label "Human Agent"@en ;
    rdfs:comment "A human evaluator performing ratings."@en .

rating:AutomatedAgent a rdfs:Class ;
    rdfs:subClassOf rating:Agent ;
    rdfs:label "Automated Agent"@en ;
    rdfs:comment "An automated system or model performing ratings."@en .

rating:Criterion a rdfs:Class ;
    rdfs:label "Criterion"@en ;
    rdfs:comment "A specific aspect or dimension being evaluated."@en .

rating:Scale a rdfs:Class ;
    rdfs:label "Scale"@en ;
    rdfs:comment "A measurement scale defining possible rating values."@en .

#
# ╭──────────────────────────────────────────────────╮
# │ Core Relationships                               │
# ╰──────────────────────────────────────────────────╯
#

rating:contains a rdf:Property ;
    rdfs:label "contains"@en ;
    rdfs:comment "Links an evaluation to the ratings it contains."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range rating:Rating .

rating:evaluates a rdf:Property ;
    rdfs:label "evaluates"@en ;
    rdfs:comment "Points to the resource being evaluated."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range rdfs:Resource .

rating:performedBy a rdf:Property ;
    rdfs:label "performed by"@en ;
    rdfs:comment "Identifies the agent who performed the rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range rating:Agent .

rating:accordingTo a rdf:Property ;
    rdfs:label "according to"@en ;
    rdfs:comment "Specifies the criterion used for the rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range rating:Criterion .

rating:onScale a rdf:Property ;
    rdfs:label "on scale"@en ;
    rdfs:comment "Specifies the scale on which the rating is given."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range rating:Scale .

#
# ╭──────────────────────────────────────────────────╮
# │ Rating Properties                                │
# ╰──────────────────────────────────────────────────╯
#

rating:hasValue a rdf:Property ;
    rdfs:label "has value"@en ;
    rdfs:comment "The actual rating value assigned."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range rdfs:Literal .

rating:hasRationale a rdf:Property ;
    rdfs:label "has rationale"@en ;
    rdfs:comment "Textual explanation or justification for the rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:string .

rating:confidence a rdf:Property ;
    rdfs:label "confidence"@en ;
    rdfs:comment "Confidence level of the rating agent in their assessment."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:decimal .

# Additional rating properties for comprehensive evaluation support
rating:score a rdf:Property ;
    rdfs:label "score"@en ;
    rdfs:comment "Numeric score assigned in the rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:decimal .

rating:feedback a rdf:Property ;
    rdfs:label "feedback"@en ;
    rdfs:comment "Textual feedback or rationale for the rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:string .

# DEPRECATED: Use rating:accordingTo instead
rating:metric a rdf:Property ;
    rdfs:label "metric"@en ;
    rdfs:comment "Identifier of the metric being measured."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:string .

# DEPRECATED: Use rating:performedBy with AutomatedAgent instead
rating:generatedBy a rdf:Property ;
    rdfs:label "generated by"@en ;
    rdfs:comment "Direct reference to the LLM model that generated this rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:string .

rating:withTemperature a rdf:Property ;
    rdfs:label "with temperature"@en ;
    rdfs:comment "Temperature parameter used for this specific rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:decimal .

rating:withMaxTokens a rdf:Property ;
    rdfs:label "with max tokens"@en ;
    rdfs:comment "Max tokens parameter used for this specific rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:integer .

rating:usingPrompt a rdf:Property ;
    rdfs:label "using prompt"@en ;
    rdfs:comment "Prompt template identifier used for this rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:string .

rating:partOfEvaluation a rdf:Property ;
    rdfs:label "part of evaluation"@en ;
    rdfs:comment "Links a rating back to its parent evaluation for grouping."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range rating:Evaluation .

rating:status a rdf:Property ;
    rdfs:label "status"@en ;
    rdfs:comment "Status of the rating (SUCCESS, FAILED, PENDING)."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:string .

rating:ratedEntity a rdf:Property ;
    rdfs:label "rated entity"@en ;
    rdfs:comment "The specific entity being rated."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range rdfs:Resource .

# Token usage properties for LLM-based ratings
rating:inputTokens a rdf:Property ;
    rdfs:label "input tokens"@en ;
    rdfs:comment "Number of input tokens used by the LLM for this rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:integer .

rating:outputTokens a rdf:Property ;
    rdfs:label "output tokens"@en ;
    rdfs:comment "Number of output tokens generated by the LLM for this rating."@en ;
    rdfs:domain rating:Rating ;
    rdfs:range xsd:integer .

#
# ╭──────────────────────────────────────────────────╮
# │ Evaluation Properties                            │
# ╰──────────────────────────────────────────────────╯
#

rating:startedAt a rdf:Property ;
    rdfs:label "started at"@en ;
    rdfs:comment "Timestamp when the evaluation began."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:dateTime .

rating:completedAt a rdf:Property ;
    rdfs:label "completed at"@en ;
    rdfs:comment "Timestamp when the evaluation was completed."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:dateTime .

rating:purpose a rdf:Property ;
    rdfs:label "purpose"@en ;
    rdfs:comment "The intended purpose or goal of the evaluation."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:string .

rating:methodology a rdf:Property ;
    rdfs:label "methodology"@en ;
    rdfs:comment "Description of the evaluation methodology used."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:string .

# Task and session tracking properties
rating:taskId a rdf:Property ;
    rdfs:label "task ID"@en ;
    rdfs:comment "Identifier of the task associated with this evaluation."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:string .

rating:executedAt a rdf:Property ;
    rdfs:label "executed at"@en ;
    rdfs:comment "Timestamp when the evaluation was executed."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:dateTime .

rating:hasRating a rdf:Property ;
    rdfs:label "has rating"@en ;
    rdfs:comment "Links an evaluation to its constituent ratings."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range rating:Rating .

# Experiment tracking
rating:experimentName a rdf:Property ;
    rdfs:label "experiment name"@en ;
    rdfs:comment "Name of the experiment this evaluation is part of."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:string .

# LLM Configuration Properties for Evaluations
rating:modelName a rdf:Property ;
    rdfs:label "model name"@en ;
    rdfs:comment "Name of the LLM model used for evaluation."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:string .

rating:modelVersion a rdf:Property ;
    rdfs:label "model version"@en ;
    rdfs:comment "Version of the LLM model used."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:string .

rating:temperature a rdf:Property ;
    rdfs:label "temperature"@en ;
    rdfs:comment "Temperature parameter used for LLM generation."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:decimal .

rating:maxTokens a rdf:Property ;
    rdfs:label "max tokens"@en ;
    rdfs:comment "Maximum number of tokens for LLM generation."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:integer .

rating:timeout a rdf:Property ;
    rdfs:label "timeout"@en ;
    rdfs:comment "Timeout value in milliseconds for LLM requests."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:integer .

# Prompt Configuration Properties
rating:promptTemplate a rdf:Property ;
    rdfs:label "prompt template"@en ;
    rdfs:comment "Identifier of the prompt template used."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:string .

rating:promptVersion a rdf:Property ;
    rdfs:label "prompt version"@en ;
    rdfs:comment "Version of the prompt template used."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:integer .

# Query Configuration Properties  
rating:queryType a rdf:Property ;
    rdfs:label "query type"@en ;
    rdfs:comment "Type of query used to extract data for evaluation."@en ;
    rdfs:domain rating:Evaluation ;
    rdfs:range xsd:string .

#
# ╭──────────────────────────────────────────────────╮
# │ Agent Properties                                 │
# ╰──────────────────────────────────────────────────╯
#

# Human Agent Properties
rating:name a rdf:Property ;
    rdfs:label "name"@en ;
    rdfs:comment "Name of the human agent."@en ;
    rdfs:domain rating:HumanAgent ;
    rdfs:range xsd:string .

rating:affiliation a rdf:Property ;
    rdfs:label "affiliation"@en ;
    rdfs:comment "Organizational affiliation of the human agent."@en ;
    rdfs:domain rating:HumanAgent ;
    rdfs:range xsd:string .

rating:expertise a rdf:Property ;
    rdfs:label "expertise"@en ;
    rdfs:comment "Domain expertise of the human agent."@en ;
    rdfs:domain rating:HumanAgent ;
    rdfs:range xsd:string .

# Automated Agent Properties
rating:model a rdf:Property ;
    rdfs:label "model"@en ;
    rdfs:comment "Model identifier for the automated agent."@en ;
    rdfs:domain rating:AutomatedAgent ;
    rdfs:range xsd:string .

rating:provider a rdf:Property ;
    rdfs:label "provider"@en ;
    rdfs:comment "Service provider or endpoint for the automated agent (e.g., openrouter, openai)."@en ;
    rdfs:domain rating:AutomatedAgent ;
    rdfs:range xsd:string .

rating:temperature a rdf:Property ;
    rdfs:label "temperature"@en ;
    rdfs:comment "Temperature parameter for LLM generation."@en ;
    rdfs:domain rating:AutomatedAgent ;
    rdfs:range xsd:decimal .

rating:maxTokens a rdf:Property ;
    rdfs:label "max tokens"@en ;
    rdfs:comment "Maximum tokens parameter for LLM generation."@en ;
    rdfs:domain rating:AutomatedAgent ;
    rdfs:range xsd:integer .

#
# ╭──────────────────────────────────────────────────╮
# │ Criterion Properties                             │
# ╰──────────────────────────────────────────────────╯
#

rating:measures a rdf:Property ;
    rdfs:label "measures"@en ;
    rdfs:comment "Description of what aspect this criterion measures."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

rating:instruction a rdf:Property ;
    rdfs:label "instruction"@en ;
    rdfs:comment "Instructions for applying this criterion."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

rating:category a rdf:Property ;
    rdfs:label "category"@en ;
    rdfs:comment "Category or domain of the criterion."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

# Metric-specific properties for comprehensive criterion definition
rating:metricId a rdf:Property ;
    rdfs:label "metric ID"@en ;
    rdfs:comment "Unique identifier for the metric criterion."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

rating:metricType a rdf:Property ;
    rdfs:label "metric type"@en ;
    rdfs:comment "Type classification of the metric (rating, measurement, etc.)."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

rating:scale a rdf:Property ;
    rdfs:label "scale"@en ;
    rdfs:comment "Scale definition for the metric (e.g., '0-10', 'categorical')."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

# RDF mapping properties for semantic integration
rating:rdfType a rdf:Property ;
    rdfs:label "RDF type"@en ;
    rdfs:comment "RDF type that this criterion maps to in the domain ontology."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

rating:rdfDomain a rdf:Property ;
    rdfs:label "RDF domain"@en ;
    rdfs:comment "RDF domain classification for semantic grouping."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

# Role-based evaluation properties
rating:roleId a rdf:Property ;
    rdfs:label "role ID"@en ;
    rdfs:comment "Identifier of the evaluator role for this criterion."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

rating:roleVersion a rdf:Property ;
    rdfs:label "role version"@en ;
    rdfs:comment "Version of the evaluator role definition."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:integer .

# Field usage for data mapping
rating:usesField a rdf:Property ;
    rdfs:label "uses field"@en ;
    rdfs:comment "Specifies a data field that this criterion evaluates."@en ;
    rdfs:domain rating:Criterion ;
    rdfs:range xsd:string .

#
# ╭──────────────────────────────────────────────────╮
# │ Scale Properties                                 │
# ╰──────────────────────────────────────────────────╯
#

rating:minValue a rdf:Property ;
    rdfs:label "minimum value"@en ;
    rdfs:comment "Minimum value on this scale."@en ;
    rdfs:domain rating:Scale ;
    rdfs:range rdfs:Literal .

rating:maxValue a rdf:Property ;
    rdfs:label "maximum value"@en ;
    rdfs:comment "Maximum value on this scale."@en ;
    rdfs:domain rating:Scale ;
    rdfs:range rdfs:Literal .

rating:scaleType a rdf:Property ;
    rdfs:label "scale type"@en ;
    rdfs:comment "Type of scale (ordinal, interval, ratio, categorical)."@en ;
    rdfs:domain rating:Scale ;
    rdfs:range xsd:string .

rating:unit a rdf:Property ;
    rdfs:label "unit"@en ;
    rdfs:comment "Unit of measurement for the scale values."@en ;
    rdfs:domain rating:Scale ;
    rdfs:range xsd:string .