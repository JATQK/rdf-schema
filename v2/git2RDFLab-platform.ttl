@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix git: <https://purl.archive.org/git2rdf/v2/git2RDFLab-git#> .
@prefix platform: <https://purl.archive.org/git2rdf/v2/git2RDFLab-platform#> .

#
# ╭──────────────────────────────────────────────────╮
# │ Metadata                                         │
# ╰──────────────────────────────────────────────────╯
#

<https://purl.archive.org/git2rdf/v2/git2RDFLab-platform#> a owl:Ontology ;
    owl:versionInfo "2.0" ;
    owl:imports <https://purl.archive.org/git2rdf/v2/git2RDFLab-git#> ,
                <http://www.w3.org/ns/prov#> ,
                <http://xmlns.com/foaf/0.1/> ;
    rdfs:label "Git2RDF Platform Ontology"@en ;
    rdfs:comment "Minimal platform concepts for Git hosting platforms"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Repository                                │
# ╰──────────────────────────────────────────────────╯
#

platform:Repository a rdfs:Class ;
    rdfs:subClassOf git:GitRepository, prov:Entity ;
    rdfs:label "Repository"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Ticket                                    │
# ╰──────────────────────────────────────────────────╯
#

platform:Ticket a rdfs:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "Ticket"@en ;
    rdfs:comment "Issues, pull requests, etc."@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: PullRequest                               │
# ╰──────────────────────────────────────────────────╯
#

platform:PullRequest a rdfs:Class ;
    rdfs:subClassOf platform:Ticket ;
    rdfs:label "Pull Request"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Person                                    │
# ╰──────────────────────────────────────────────────╯
#

platform:Person a rdfs:Class ;
    owl:equivalentClass git:Person ;
    rdfs:label "Person"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Comment                                   │
# ╰──────────────────────────────────────────────────╯
#

platform:Comment a rdfs:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "Comment"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Label                                     │
# ╰──────────────────────────────────────────────────╯
#

platform:Label a rdfs:Class ;
    rdfs:label "Label"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Workflow                                  │
# ╰──────────────────────────────────────────────────╯
#

platform:Workflow a rdfs:Class ;
    rdfs:label "Workflow"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: WorkflowExecution                         │
# ╰──────────────────────────────────────────────────╯
#

platform:WorkflowExecution a rdfs:Class ;
    rdfs:subClassOf prov:Activity ;
    rdfs:label "Workflow Execution"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: WorkflowStep                              │
# ╰──────────────────────────────────────────────────╯
#

platform:WorkflowStep a rdfs:Class ;
    rdfs:subClassOf prov:Activity ;
    rdfs:label "Workflow Step"@en ;
    rdfs:comment "A single step within a workflow execution"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Analyzable                                │
# ╰──────────────────────────────────────────────────╯
#

platform:Analyzable a rdfs:Class ;
    rdfs:label "Analyzable Resource"@en ;
    rdfs:comment "Resources that can have analysis results, ratings, metrics attached"@en ;
    owl:unionOf (platform:Repository platform:Ticket platform:PullRequest platform:Comment platform:Review platform:ReviewComment platform:Reaction platform:WorkflowExecution git:GitCommit) .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Rating                                    │
# ╰──────────────────────────────────────────────────╯
#

platform:Rating a rdfs:Class ;
    rdfs:label "Rating"@en ;
    rdfs:comment "A rating or score assigned to a resource"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Metrics                                   │
# ╰──────────────────────────────────────────────────╯
#

platform:Metrics a rdfs:Class ;
    rdfs:label "Metrics"@en ;
    rdfs:comment "Statistical metrics about a resource"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Class: Evaluation                                │
# ╰──────────────────────────────────────────────────╯
#

platform:Evaluation a rdfs:Class ;
    rdfs:label "Evaluation"@en ;
    rdfs:comment "An evaluation or assessment of a resource"@en .


#
# ╭──────────────────────────────────────────────────╮
# │ Universal Properties                             │
# ╰──────────────────────────────────────────────────╯
#
platform:id a rdf:Property ;
    rdfs:label "ID"@en ;
    rdfs:comment "Platform-specific unique identifier. Format varies by platform (GitHub: integer, GitLab: integer, etc.)"@en ;
    rdfs:range xsd:string .

platform:runId a rdf:Property ;
    rdfs:label "Run ID"@en ;
    rdfs:comment "Platform-specific workflow or pipeline run identifier"@en ;
    rdfs:domain platform:WorkflowExecution ;
    rdfs:range xsd:string .

platform:name a rdf:Property ;
    rdfs:label "Name"@en ;
    rdfs:comment "Display name of the resource. Should be human-readable"@en ;
    rdfs:range xsd:string .

platform:title a rdf:Property ;
    rdfs:label "Title"@en ;
    rdfs:comment "Title of the resource. Should be concise and descriptive"@en ;
    rdfs:range xsd:string .

platform:description a rdf:Property ;
    rdfs:label "Description"@en ;
    rdfs:comment "Detailed description of the resource. May contain markdown formatting"@en ;
    rdfs:range xsd:string .

platform:body a rdf:Property ;
    rdfs:label "Body"@en ;
    rdfs:comment "Main text content. May contain markdown formatting"@en ;
    rdfs:range xsd:string .

platform:state a rdf:Property ;
    rdfs:label "State"@en ;
    rdfs:comment "Expected values: 'open', 'closed', 'merged', 'draft' (tickets/PRs); platform-specific extensions allowed like platform:open platform:closed platform:merged platform:draft "@en ;
    rdfs:range xsd:string .

platform:url a rdf:Property ;
    rdfs:label "URL"@en ;
    rdfs:range xsd:anyURI .


#
# ╭──────────────────────────────────────────────────╮
# │ Temporal Properties                              │
# ╰──────────────────────────────────────────────────╯
#

platform:createdAt a rdf:Property ;
    rdfs:subPropertyOf prov:generatedAtTime ;
    rdfs:label "Created At"@en ;
    rdfs:range xsd:dateTime .

platform:updatedAt a rdf:Property ;
    rdfs:subPropertyOf prov:generatedAtTime ;
    rdfs:label "Updated At"@en ;
    rdfs:range xsd:dateTime .

platform:closedAt a rdf:Property ;
    rdfs:subPropertyOf prov:invalidatedAtTime ;
    rdfs:label "Closed At"@en ;
    rdfs:range xsd:dateTime .


#
# ╭──────────────────────────────────────────────────╮
# │ Person Properties                                │
# ╰──────────────────────────────────────────────────╯
#

platform:username a rdf:Property ;
    rdfs:label "Username"@en ;
    rdfs:comment "Platform username/login. Should be unique within the platform"@en ;
    rdfs:domain platform:Person ;
    rdfs:range xsd:string .

platform:email a rdf:Property ;
    rdfs:label "Email"@en ;
    rdfs:comment "Valid email address format (RFC 5322 compliant)"@en ;
    rdfs:domain platform:Person ;
    rdfs:range xsd:string .


#
# ╭──────────────────────────────────────────────────╮
# │ Ticket Properties                                │
# ╰──────────────────────────────────────────────────╯
#

platform:number a rdf:Property ;
    rdfs:label "Number"@en ;
    rdfs:domain platform:Ticket ;
    rdfs:range xsd:nonNegativeInteger .

platform:submitter a rdf:Property ;
    rdfs:subPropertyOf prov:wasAttributedTo ;
    rdfs:label "Submitter"@en ;
    rdfs:domain platform:Ticket ;
    rdfs:range platform:Person .

platform:submitterOf a rdf:Property ;
    owl:inverseOf platform:submitter ;
    rdfs:label "Submitter Of"@en ;
    rdfs:comment "Find all tickets submitted by this person"@en ;
    rdfs:domain platform:Person ;
    rdfs:range platform:Ticket .

platform:assignee a rdf:Property ;
    rdfs:label "Assignee"@en ;
    rdfs:domain platform:Ticket ;
    rdfs:range platform:Person .

platform:assigneeOf a rdf:Property ;
    owl:inverseOf platform:assignee ;
    rdfs:label "Assignee Of"@en ;
    rdfs:comment "Find all tickets assigned to this person"@en ;
    rdfs:domain platform:Person ;
    rdfs:range platform:Ticket .

platform:locked a rdf:Property ;
    rdfs:label "Locked"@en ;
    rdfs:domain platform:Ticket ;
    rdfs:range xsd:boolean .

platform:draft a rdf:Property ;
    rdfs:label "Draft"@en ;
    rdfs:domain platform:Ticket ;
    rdfs:range xsd:boolean .


#
# ╭──────────────────────────────────────────────────╮
# │ PullRequest Properties                           │
# ╰──────────────────────────────────────────────────╯
#

platform:sourceBranch a rdf:Property ;
    rdfs:label "Source Branch"@en ;
    rdfs:domain platform:PullRequest ;
    rdfs:range git:GitBranch .

platform:targetBranch a rdf:Property ;
    rdfs:label "Target Branch"@en ;
    rdfs:domain platform:PullRequest ;
    rdfs:range git:GitBranch .

platform:merged a rdf:Property ;
    rdfs:label "Merged"@en ;
    rdfs:domain platform:PullRequest ;
    rdfs:range xsd:boolean .

platform:mergedBy a rdf:Property ;
    rdfs:label "Merged By"@en ;
    rdfs:domain platform:PullRequest ;
    rdfs:range platform:Person .

platform:mergedByOf a rdf:Property ;
    owl:inverseOf platform:mergedBy ;
    rdfs:label "Merged By Of"@en ;
    rdfs:comment "Find all pull requests merged by this person"@en ;
    rdfs:domain platform:Person ;
    rdfs:range platform:PullRequest .

platform:mergedAt a rdf:Property ;
    rdfs:subPropertyOf prov:endedAtTime ;
    rdfs:label "Merged At"@en ;
    rdfs:domain platform:PullRequest ;
    rdfs:range xsd:dateTime .

platform:mergeCommitSha a rdf:Property ;
    rdfs:label "Merge Commit SHA"@en ;
    rdfs:comment "Git commit SHA hash (40-character hexadecimal string)"@en ;
    rdfs:domain platform:PullRequest ;
    rdfs:range xsd:string .


#
# ╭──────────────────────────────────────────────────╮
# │ Repository Properties                            │
# ╰──────────────────────────────────────────────────╯
#

platform:owner a rdf:Property ;
    rdfs:label "Owner"@en ;
    rdfs:domain platform:Repository ;
    rdfs:range platform:Person .

platform:ownerOf a rdf:Property ;
    owl:inverseOf platform:owner ;
    rdfs:label "Owner Of"@en ;
    rdfs:comment "Find all repositories owned by this person"@en ;
    rdfs:domain platform:Person ;
    rdfs:range platform:Repository .

platform:isPrivate a rdf:Property ;
    rdfs:label "Is Private"@en ;
    rdfs:domain platform:Repository ;
    rdfs:range xsd:boolean .

platform:defaultBranch a rdf:Property ;
    rdfs:label "Default Branch"@en ;
    rdfs:domain platform:Repository ;
    rdfs:range git:GitBranch .


#
# ╭──────────────────────────────────────────────────╮
# │ Comment Properties                               │
# ╰──────────────────────────────────────────────────╯
#

platform:author a rdf:Property ;
    rdfs:subPropertyOf prov:wasAttributedTo ;
    rdfs:label "Author"@en ;
    rdfs:domain platform:Comment ;
    rdfs:range platform:Person .

platform:authorOf a rdf:Property ;
    owl:inverseOf platform:author ;
    rdfs:label "Author Of"@en ;
    rdfs:comment "Find all comments authored by this person"@en ;
    rdfs:domain platform:Person ;
    rdfs:range platform:Comment .


#
# ╭──────────────────────────────────────────────────╮
# │ Workflow Properties                              │
# ╰──────────────────────────────────────────────────╯
#

platform:trigger a rdf:Property ;
    rdfs:label "Trigger"@en ;
    rdfs:comment "Expected values: 'push', 'pull_request', 'schedule', 'manual', 'workflow_dispatch', 'merge_request'"@en ;
    rdfs:domain platform:WorkflowExecution ;
    rdfs:range xsd:string .

platform:status a rdf:Property ;
    rdfs:label "Status"@en ;
    rdfs:comment "Expected values: 'queued', 'in_progress', 'completed', 'pending', 'running', 'waiting'"@en ;
    rdfs:domain platform:WorkflowExecution ;
    rdfs:range xsd:string .

platform:conclusion a rdf:Property ;
    rdfs:label "Conclusion"@en ;
    rdfs:comment "Expected values: 'success', 'failure', 'cancelled', 'skipped', 'timed_out', 'action_required'"@en ;
    rdfs:domain platform:WorkflowExecution ;
    rdfs:range xsd:string .

platform:runAttempt a rdf:Property ;
    rdfs:label "Run Attempt"@en ;
    rdfs:comment "Attempt number for workflow execution or step"@en ;
    rdfs:range xsd:positiveInteger .

platform:jobUrl a rdf:Property ;
    rdfs:label "Job URL"@en ;
    rdfs:comment "URL to the specific job or step within the workflow execution"@en ;
    rdfs:range xsd:anyURI .


#
# ╭──────────────────────────────────────────────────╮
# │ Hierarchical Relationships                       │
# ╰──────────────────────────────────────────────────╯
#

platform:hasLabel a rdf:Property ;
    rdfs:label "Has Label"@en ;
    rdfs:range platform:Label .

platform:labelOf a rdf:Property ;
    owl:inverseOf platform:hasLabel ;
    rdfs:label "Label Of"@en ;
    rdfs:domain platform:Label .

platform:hasComment a rdf:Property ;
    rdfs:label "Has Comment"@en ;
    rdfs:range platform:Comment .

platform:commentOf a rdf:Property ;
    owl:inverseOf platform:hasComment ;
    rdfs:label "Comment Of"@en ;
    rdfs:domain platform:Comment .

platform:containsCommit a rdf:Property ;
    rdfs:label "Contains Commit"@en ;
    rdfs:domain platform:Ticket ;
    rdfs:range git:GitCommit .

platform:partOfTicket a rdf:Property ;
    owl:inverseOf platform:containsCommit ;
    rdfs:label "Part Of Ticket"@en ;
    rdfs:domain git:GitCommit ;
    rdfs:range platform:Ticket .

platform:hasExecution a rdf:Property ;
    rdfs:label "Has Execution"@en ;
    rdfs:domain platform:Workflow ;
    rdfs:range platform:WorkflowExecution .

platform:executionOf a rdf:Property ;
    owl:inverseOf platform:hasExecution ;
    rdfs:label "Execution Of"@en ;
    rdfs:domain platform:WorkflowExecution ;
    rdfs:range platform:Workflow .


#
# ╭──────────────────────────────────────────────────╮
# │ Analysis Properties                              │
# ╰──────────────────────────────────────────────────╯
#

platform:hasAttachment a rdf:Property ;
    rdfs:label "Has Attachment"@en ;
    rdfs:comment "Generic property for connecting resources to external analysis/evaluation results"@en ;
    rdfs:domain platform:Analyzable .

platform:hasRating a rdf:Property ;
    rdfs:subPropertyOf platform:hasAttachment ;
    rdfs:label "Has Rating"@en ;
    rdfs:domain platform:Analyzable ;
    rdfs:range platform:Rating .

platform:ratingOf a rdf:Property ;
    owl:inverseOf platform:hasRating ;
    rdfs:label "Rating Of"@en ;
    rdfs:comment "Find what resource this rating applies to"@en ;
    rdfs:domain platform:Rating ;
    rdfs:range platform:Analyzable .

platform:hasEvaluation a rdf:Property ;
    rdfs:subPropertyOf platform:hasAttachment ;
    rdfs:label "Has Evaluation"@en ;
    rdfs:domain platform:Analyzable ;
    rdfs:range platform:Evaluation .

platform:evaluationOf a rdf:Property ;
    owl:inverseOf platform:hasEvaluation ;
    rdfs:label "Evaluation Of"@en ;
    rdfs:comment "Find what resource this evaluation applies to"@en ;
    rdfs:domain platform:Evaluation ;
    rdfs:range platform:Analyzable .

platform:hasMetrics a rdf:Property ;
    rdfs:subPropertyOf platform:hasAttachment ;
    rdfs:label "Has Metrics"@en ;
    rdfs:domain platform:Analyzable ;
    rdfs:range platform:Metrics .

platform:metricsOf a rdf:Property ;
    owl:inverseOf platform:hasMetrics ;
    rdfs:label "Metrics Of"@en ;
    rdfs:comment "Find what resource these metrics apply to"@en ;
    rdfs:domain platform:Metrics ;
    rdfs:range platform:Analyzable .

platform:hasAnalysisResult a rdf:Property ;
    rdfs:subPropertyOf platform:hasAttachment ;
    rdfs:label "Has Analysis Result"@en ;
    rdfs:domain platform:Analyzable .

platform:analysisResultOf a rdf:Property ;
    owl:inverseOf platform:hasAnalysisResult ;
    rdfs:label "Analysis Result Of"@en ;
    rdfs:comment "Find what resource this analysis result applies to"@en ;
    rdfs:range platform:Analyzable .

platform:analysisType a rdf:Property ;
    rdfs:label "Analysis Type"@en ;
    rdfs:comment "Type of analysis, rating, or evaluation. Expected values: 'code_quality', 'security_scan', 'performance', 'complexity', 'coverage', 'sentiment', 'spam_detection', 'risk_assessment'"@en ;
    rdfs:range xsd:string .

#
# ╭──────────────────────────────────────────────────╮
# │ Review Classes                                   │
# ╰──────────────────────────────────────────────────╯
#

platform:Review a rdfs:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "Review"@en ;
    rdfs:comment "A code review submission for a pull/merge request"@en .

platform:ReviewComment a rdfs:Class ;
    rdfs:subClassOf platform:Comment ;
    rdfs:label "Review Comment"@en ;
    rdfs:comment "A comment made during code review"@en .

platform:ReviewState a rdfs:Class ;
    rdfs:label "Review State"@en ;
    rdfs:comment "The current state of a review"@en .

platform:Reaction a rdfs:Class ;
    rdfs:subClassOf prov:Entity ;
    rdfs:label "Reaction"@en ;
    rdfs:comment "An emoji reaction to a resource (issue, PR, comment, etc.)"@en .

platform:ReactionType a rdfs:Class ;
    rdfs:label "Reaction Type"@en ;
    rdfs:comment "The type of emoji reaction"@en .

#
# ╭──────────────────────────────────────────────────╮
# │ Review Properties                                │
# ╰──────────────────────────────────────────────────╯
#

platform:reviewer a rdf:Property ;
    rdfs:label "Reviewer"@en ;
    rdfs:comment "Person who is performing the review"@en ;
    rdfs:domain platform:Review ;
    rdfs:range platform:Person .

platform:reviewerOf a rdf:Property ;
    owl:inverseOf platform:reviewer ;
    rdfs:label "Reviewer Of"@en ;
    rdfs:comment "Find all reviews performed by this person"@en ;
    rdfs:domain platform:Person ;
    rdfs:range platform:Review .

platform:reviewState a rdf:Property ;
    rdfs:label "Review State"@en ;
    rdfs:comment "Current state of the review (approved, changes_requested, commented, etc.)"@en ;
    rdfs:domain platform:Review ;
    rdfs:range platform:ReviewState .

platform:reviewedAt a rdf:Property ;
    rdfs:subPropertyOf prov:generatedAtTime ;
    rdfs:label "Reviewed At"@en ;
    rdfs:comment "When the review was submitted"@en ;
    rdfs:domain platform:Review ;
    rdfs:range xsd:dateTime .

platform:reviewBody a rdf:Property ;
    rdfs:label "Review Body"@en ;
    rdfs:comment "Main text content of the review"@en ;
    rdfs:domain platform:Review ;
    rdfs:range xsd:string .

# Review Request Properties (on PullRequest)
platform:requestedReviewer a rdf:Property ;
    rdfs:label "Requested Reviewer"@en ;
    rdfs:comment "Person who has been requested to review this pull request"@en ;
    rdfs:domain platform:PullRequest ;
    rdfs:range platform:Person .

# Review-PullRequest Relationships
platform:hasReview a rdf:Property ;
    rdfs:label "Has Review"@en ;
    rdfs:comment "Pull request has this review"@en ;
    rdfs:domain platform:PullRequest ;
    rdfs:range platform:Review .

platform:reviewOf a rdf:Property ;
    owl:inverseOf platform:hasReview ;
    rdfs:label "Review Of"@en ;
    rdfs:comment "This review belongs to a pull request"@en ;
    rdfs:domain platform:Review ;
    rdfs:range platform:PullRequest .

# Review Comment Relationships
platform:hasReviewComment a rdf:Property ;
    rdfs:label "Has Review Comment"@en ;
    rdfs:comment "Review has this comment"@en ;
    rdfs:domain platform:Review ;
    rdfs:range platform:ReviewComment .

platform:reviewCommentOf a rdf:Property ;
    owl:inverseOf platform:hasReviewComment ;
    rdfs:label "Review Comment Of"@en ;
    rdfs:comment "This comment belongs to a review"@en ;
    rdfs:domain platform:ReviewComment ;
    rdfs:range platform:Review .

# Review Comment Threading
platform:inReplyTo a rdf:Property ;
    rdfs:label "In Reply To"@en ;
    rdfs:comment "This review comment is a reply to another review comment"@en ;
    rdfs:domain platform:ReviewComment ;
    rdfs:range platform:ReviewComment .

platform:hasReply a rdf:Property ;
    owl:inverseOf platform:inReplyTo ;
    rdfs:label "Has Reply"@en ;
    rdfs:comment "This review comment has replies"@en ;
    rdfs:domain platform:ReviewComment ;
    rdfs:range platform:ReviewComment .

#
# ╭──────────────────────────────────────────────────╮
# │ Review State Enumeration Values                  │
# ╰──────────────────────────────────────────────────╯
#

platform:COMMENTED a platform:ReviewState ;
    rdfs:label "Commented"@en ;
    rdfs:comment "General feedback provided without explicit approval/rejection"@en .

platform:APPROVED a platform:ReviewState ;
    rdfs:label "Approved"@en ;
    rdfs:comment "Changes have been approved for merging"@en .

platform:CHANGES_REQUESTED a platform:ReviewState ;
    rdfs:label "Changes Requested"@en ;
    rdfs:comment "Changes must be made before approval"@en .

platform:DISMISSED a platform:ReviewState ;
    rdfs:label "Dismissed"@en ;
    rdfs:comment "Review was dismissed due to new commits or other reasons"@en .

platform:PENDING a platform:ReviewState ;
    rdfs:label "Pending"@en ;
    rdfs:comment "Review is pending submission"@en .

#
# ╭──────────────────────────────────────────────────╮
# │ Reaction Properties                              │
# ╰──────────────────────────────────────────────────╯
#

# Core Reaction Properties
platform:reactedBy a rdf:Property ;
    rdfs:label "Reacted By"@en ;
    rdfs:comment "Person who made the reaction"@en ;
    rdfs:domain platform:Reaction ;
    rdfs:range platform:Person .

platform:reacted a rdf:Property ;
    owl:inverseOf platform:reactedBy ;
    rdfs:label "Reacted"@en ;
    rdfs:comment "Find all reactions made by this person"@en ;
    rdfs:domain platform:Person ;
    rdfs:range platform:Reaction .

platform:reactionType a rdf:Property ;
    rdfs:label "Reaction Type"@en ;
    rdfs:comment "The type of emoji reaction (platform-specific values)"@en ;
    rdfs:domain platform:Reaction ;
    rdfs:range xsd:string .

platform:reactedAt a rdf:Property ;
    rdfs:subPropertyOf prov:generatedAtTime ;
    rdfs:label "Reacted At"@en ;
    rdfs:comment "When the reaction was made"@en ;
    rdfs:domain platform:Reaction ;
    rdfs:range xsd:dateTime .

# Reaction Relationships (can react to various resources)
platform:hasReaction a rdf:Property ;
    rdfs:label "Has Reaction"@en ;
    rdfs:comment "Resource has this reaction"@en ;
    rdfs:range platform:Reaction .

platform:reactionOf a rdf:Property ;
    owl:inverseOf platform:hasReaction ;
    rdfs:label "Reaction Of"@en ;
    rdfs:comment "This reaction belongs to a resource"@en ;
    rdfs:domain platform:Reaction .